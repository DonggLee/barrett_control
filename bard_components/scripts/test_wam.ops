import("bard_components")
loadComponent("my_wam","bard_components::WAM"
loadComponent("con_trivial","bard_components::controllers::trivial")
var ConnPolicy cp
cp.type=BUFFER
cp.size=10
connect("my_wam.torques_in","con_trivial.torques_out",cp)
con_trivial.setPeriod(0.001)
con_trivial.activate
con_trivial.start
my_wam.start
